<?xml version="1.0"?>
<launch>
  <arg name="onto" default="$(find mros1_reasoner)/scripts/kb.owl"/>
  <arg name="desired_configuration" default="f3_v3_r1"/>

  <param name="/onto_file" type="string" value="$(arg onto)"/>
  <param name="/desired_configuration" type="string" value="$(arg desired_configuration)"/>
  <param name="/nfr_energy" value="0.4"/>
  <param name="/nfr_safety" value="0.4"/>
  <node name="reasoner" pkg="mros1_reasoner" type="mros1_reasoner.py" output="screen"/>
  <test test-name="send_QA_value_test" pkg="mros1_reasoner" type="send_qa_value_test.py" />
</launch>